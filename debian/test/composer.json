{
    "name": "deb/ease-html-test",
    "description": "Development files for EaseHtml",
    "version": "0.5",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ease\\": "\\src\\Ease",
            "Ease\\Html\\": "src\\EaseHtml",
            "Test\\Ease\\": [ "/usr/share/php/EaseCore/Test/Ease", "/usr/share/php/EaseHtml/Test" ],
            "Test\\Ease\\Html\\": "/usr/share/php/EaseHtml/Test/Html"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "deb/ease-core": "*",
        "deb/ease-html": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/EaseCore",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseHtml",
            "options": {
                "symlink": true
            }
        }
    ],
    "type": "library",
    "license": "GPL-2.0+"
}
